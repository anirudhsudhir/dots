{
  "autosave": "on_focus_change",
  "buffer_font_family": "Ligalex Mono",
  "buffer_font_size": 15,
  "experimental.theme_overrides": {
    "syntax": {
      "comment": {
        "font_style": "italic"
      },
      "keyword": {
        "font_style": "italic"
      }
    }
  },
  "features": {
    "inline_completion_provider": "none"
  },
  "format_on_save": "on",
  "show_whitespaces": "selection",
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "theme": "Solarized Dark",
  "ui_font_size": 15,
  "vim_mode": true,
  "notification_panel": {
    "button": false
  },
  "chat_panel": {
    "button": false
  },
  "assistant": {
    "default_model": {
      "provider": "ollama",
      "model": "gemma2:2b"
    },
    "version": "2",
    "button": false
  },
  "terminal": {
    "button": false
  },
  "tab_bar": {
    "show": false
  }
}
